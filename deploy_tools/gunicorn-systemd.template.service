[Unit]
Description=Gunicorn server for SITENAME

[Service]
Restart=on-failure
User=martin
WorkingDirectory=/home/martin/sites/SITENAME/source
ExecStart=/home/martin/sites/SITENAME/virtualenv/bin/gunicorn     --bind unix:/tmp/SITENAME.socket     superlists.wsgi:application

[Install]
WantedBy=multi-user.target
